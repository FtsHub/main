local function VPIP_fake_script() -- PadoText.LocalScript 
	local script = Instance.new('LocalScript', PadoText)

	local Player = game.Players.LocalPlayer
	local label = script.Parent
	
	local function scroll(text)
		local currentText
		for i=1, #text do
			currentText = text:sub(1, i)
			label.Text = currentText
			wait(.2)
		end
	end
	
	wait(0.5)
	scroll("Welcome To Makori Hub")
	wait(3)
	scroll("Click Get Key To Get The Key")
	wait(5)
	scroll("Makori Hub")
	
	local hue = 0
	while wait() do
		hue += 1
		if hue > 360 then hue = 0 end
		
		label.TextColor3 = Color3.fromHSV(hue/360, .5, .9)
	end
	
	
	
	
	
	
end
coroutine.wrap(VPIP_fake_script)()
