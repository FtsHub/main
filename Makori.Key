local function RTOQOT_fake_script() -- Character.View 
	local script = Instance.new('LocalScript', Character)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	
	local VPFcam = Instance.new("Camera"); VPFcam.Parent = script.Parent.ViewportFrame
	VPFcam.CFrame = CFrame.new(0,0,0)
	script.Parent.ViewportFrame.CurrentCamera = VPFcam
	
	repeat wait() until game:IsLoaded()
	
	character.Archivable = true
	
	local ClonedChar = character:Clone()
	
	ClonedChar.Parent = script.Parent.ViewportFrame
	ClonedChar:SetPrimaryPartCFrame(CFrame.new(Vector3.new(0,0,-9.5), Vector3.new(0,0,0)))
end
coroutine.wrap(RTOQOT_fake_script)()
local function EWWQ_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local ImageLabel = script.Parent
	ImageLabel.Image = Players:GetUserThumbnailAsync(player.UserId, Enum.ThumbnailType.AvatarThumbnail, Enum.ThumbnailSize.Size420x420)
	
end
coroutine.wrap(EWWQ_fake_script)()
