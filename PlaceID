local placeId = game.PlaceId
if placeId == 2753915549 or placeId == 4442272183 or placeId == 7449423635 then
	--Blox Fruit
	loadstring(game:HttpGet('https://raw.githubusercontent.com/FtsHub/main/main/Secret.Lua'))()
	
elseif placeId == 6284583030 or placeId == 7722306047 or placeId == 12610002282 then
	--PetSimulatorX
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Rafacasari/roblox-scripts/main/psx.lua"))()
end
